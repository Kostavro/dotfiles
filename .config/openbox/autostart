# panel
(sleep 1s && tint2) &

# activate numlock
(sleep 1s && numlockx) &

# Set Wallpaper
(sleep 1s && /home/kostavro/git/scripts/wallpaper.sh) &

# start conky
(sleep 1s && conky) &

# apply redshift
(sleep 1s && redshift -O 3800) &

# darken screen
(sleep 1s && xcalib -co 70 -a) &

# start volwheel
(sleep 1s && volwheel) &

# start guake
(sleep 1s && guake) &
